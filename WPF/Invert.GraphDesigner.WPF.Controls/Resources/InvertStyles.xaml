<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Pen x:Key="GridLine" Thickness="1">
        <Pen.Brush>
            <SolidColorBrush Color="#FFBCCDDE"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="GridLineSecondary" Thickness="1">
        <Pen.Brush>
            <SolidColorBrush Color="#FF8BA3BB"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <SolidColorBrush x:Key="Tag1Background" Color="#cc161817"></SolidColorBrush>
    <Pen x:Key="Tag1Border" Thickness="12">
        <Pen.Brush>
            <SolidColorBrush Color="Black"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    
    <BitmapImage x:Key="DiagramArrowRight" UriSource="DesignerImages/DiagramArrowRight.png" CreateOptions="PreservePixelFormat" ></BitmapImage>
    <BitmapImage x:Key="DiagramArrowLeft" UriSource="DesignerImages/DiagramArrowLeft.png"></BitmapImage>
    <BitmapImage x:Key="DiagramArrowRightEmpty" UriSource="DesignerImages/DiagramArrowRightEmpty.png"></BitmapImage>
    <BitmapImage x:Key="DiagramArrowLeftEmpty" UriSource="DesignerImages/DiagramArrowLeftEmpty.png"></BitmapImage>
    
    <SolidColorBrush x:Key="Tag1Foreground" Color="#ffffffff"></SolidColorBrush>
    <SolidColorBrush x:Key="HeaderStyleForeground" Color="#ff888888"></SolidColorBrush>
    <SolidColorBrush x:Key="NodeBackgroundBackground" Color="#cc161817"></SolidColorBrush>
    <SolidColorBrush x:Key="ViewModelHeaderStyleForeground" Color="white"></SolidColorBrush>
    <ImageBrush x:Key="NodeCollapseBackground" ImageSource="DesignerImages/NodeCollapse.png"></ImageBrush>
    <ImageBrush x:Key="AddButtonStyleBackground" ImageSource="DesignerImages/Plus.png"></ImageBrush>
    <!--<ImageBrush x:Key="AddButtonStyle" ImageSource="DesignerImages/Remove.png"></ImageBrush>-->
    <ImageBrush x:Key="NodeExpandBackground" ImageSource="DesignerImages/NodeExpand.png"></ImageBrush>
    <Pen x:Key="NodeBackgroundBorder" Thickness="2">
        <Pen.Brush><SolidColorBrush Color="Black"></SolidColorBrush></Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter1Border" Thickness="3">
        <Pen.Brush>
            <SolidColorBrush Color="Yellow"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter2Border" Thickness="3">
        <Pen.Brush>
            <SolidColorBrush Color="Blue"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter3Border" Thickness="3">
        <Pen.Brush>
            <SolidColorBrush Color="LightBlue"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter4Background" Thickness="3">
        <Pen.Brush>
            <SolidColorBrush Color="#33B1A9FF"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter4Border" Thickness="2">
        <Pen.DashStyle>
            <DashStyle Offset="3"/>
        </Pen.DashStyle>
        <Pen.Brush>
            <SolidColorBrush Color="DarkGray"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter5Border" Thickness="3">
        <Pen.Brush>
            <SolidColorBrush Color="Green"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
    <Pen x:Key="BoxHighlighter6Border" Thickness="3">
        <Pen.Brush>
            <SolidColorBrush Color="Red"></SolidColorBrush>
        </Pen.Brush>
    </Pen>
