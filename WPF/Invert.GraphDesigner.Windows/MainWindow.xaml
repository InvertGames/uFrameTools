<Window x:Class="Invert.GraphDesigner.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="419" Width="799" Background="#FF424242">
    <DockPanel LastChildFill="True">
        <Menu Height="21" VerticalAlignment="Top" DockPanel.Dock="Top" Background="White">
            <MenuItem Header="MenuItem"/>
        </Menu>
        <ToolBar Height="36" VerticalAlignment="Top" DockPanel.Dock="Top" Background="White" />
        <Grid>
            <TabControl>
                <TabItem Header="TabItem">
                    <Grid Background="White"/>
                </TabItem>
            </TabControl>
        </Grid>
    </DockPanel>
</Window>
